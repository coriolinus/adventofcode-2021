use super::SnailfishNumber;

grammar;

Leaf: SnailfishNumber = <s:r"[0-9]+"> => SnailfishNumber::new_value(s.parse().expect("all values should be valid u8"));

pub Snailfish: SnailfishNumber = {
    <Leaf> => <>,
    "[" <left:Snailfish> "," <right:Snailfish> "]" => SnailfishNumber::new_pair(left, right),
};
